{
  "buildCommand": "cd frontend1 && npm run build",
  "outputDirectory": "frontend1/build",
  "installCommand": "cd frontend1 && npm install --legacy-peer-deps",
  "devCommand": "cd frontend1 && npm start",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}